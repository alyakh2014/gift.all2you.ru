{% extends 'base.html.twig' %}

{% block title %}LogRoutes index{% endblock %}

{% block body %}
    <h1>LogRoutes index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Date</th>
                <th>Request_type</th>
                <th>Page</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for log_route in log_routes %}
            <tr>
                <td>{{ log_route.id }}</td>
                <td>{{ log_route.date ? log_route.date|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ log_route.requestType }}</td>
                <td>{{ log_route.page }}</td>
                <td>
                    <a href="{{ path('log_routes_show', {'id': log_route.id}) }}">show</a>
                    <a href="{{ path('log_routes_edit', {'id': log_route.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('log_routes_new') }}">Create new</a>
{% endblock %}
